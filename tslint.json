{
  "extends": "@swo/tslint-config",
  "rules": {
    "component-selector": [true, "element", "noauth", "kebab-case"],
    "directive-selector": [true, "attribute", "noauth", "camelCase"],
    "pipe-naming": [true, "camelCase", "noauth"],
    "no-string-literal": false,
    "curly": false,
    "typedef": [true, "call-signature", "parameter", "property-declaration", "member-variable-declaration"]
  }
}
